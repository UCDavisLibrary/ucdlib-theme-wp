{# hooks are defined in classes/views.php #}
{% if not twigHooks %}
  {% set twigHooks = getTwigHooks() %}
{% endif %}
{% if hook and hook_category %}
  {% set hookArray = twigHooks[hook_category][hook] %}
  {% if hookArray is iterable %}
    {% for tempate_partial in hookArray %}
      {% include tempate_partial %}
    {% endfor %}
  {% endif %}
{% endif %}